{
  "name": "nativescript-nativescript-bottombar",
  "version": "1.0.0",
  "description": "Your awesome NativeScript plugin.",
  "main": "nativescript-bottombar",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns run ios --emulator",
    "demo.ios.device": "npm run preparedemo && cd demo && tns run ios",
    "demo.android": "npm run preparedemo && cd demo && tns run android --emulator",
    "test.ios": "cd demo && tns test ios --emulator",
    "test.ios.device": "cd demo && tns test ios",
    "test.android": "cd demo && tns test android",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-nativescript-bottombar && tns plugin add .. && tns install",
    "setup": "npm i && cd demo && npm i && cd .. && npm run build && cd demo && tns plugin add .. && cd ..",
    "postclone": "npm i && node scripts/postclone.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Shanika/nativescript-nativescript-bottombar.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "Android",
    "iOS"
  ],
  "author": {
    "name": "Shanika Wijerathna",
    "email": "shanikacj@gmail.com"
  },
  "bugs": {
    "url": "https://github.com/Shanika/nativescript-nativescript-bottombar/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/Shanika/nativescript-nativescript-bottombar",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^3.0.0 || ^3.0.0-rc.1",
    "tns-platform-declarations": "^3.0.0 || ^3.0.0-rc.1",
    "typescript": "^2.2.2",
    "prompt": "~1.0.0",
    "rimraf": "~2.5.0"
  },
  "peerDependencies": {
    "tns-core-modules": "^3.0.0 || ^3.0.0-rc.1"
  }
}
